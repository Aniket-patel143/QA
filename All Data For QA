ChromeDriver driver=new ChromeDriver();
driver.get("https://www.facebook.com");
driver.findElement(By.cssSelector("input[placeholder='Email address or phone number']")).sendKeys("aniket@1123");
driver.findElement(By.xpath("//input[@placeholder='Password']")).sendKeys("@1123");
driver.findElement(By.xpath("//button[text()='Log in']")).click();




























